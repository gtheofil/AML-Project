    "import numpy as np\n",
    "from sklearn.model_selection import train_test_split\n",
    "from sklearn.preprocessing import LabelEncoder\n",
    "\n",
    "def load_data(data_folder, test_size=0.2, random_state=42):\n",
    "    \"\"\"\n",
    "    EMGIMU\n",
    "    \n",
    "    :\n",
    "    - data_folder:  features.npy  labels.npy \n",
    "    - test_size:  ( 20%)\n",
    "    - random_state: \n",
    "\n",
    "    :\n",
    "    - X_train, X_test, y_train, y_test ()\n",
    "    \"\"\"\n",
    "    # 1. \n",
    "    features_path = os.path.join(data_folder, \"features.npy\")\n",
    "    labels_path = os.path.join(data_folder, \"labels.npy\")\n",
    "    \n",
    "    X = np.load(features_path)  # (num_samples, 15, 10)\n",
    "    y = np.load(labels_path)  # (num_samples,)\n",
    "\n",
    "    print(f\"Loaded features: {X.shape}, labels: {y.shape}\")\n",
    "\n",
    "    # 2.  (A0, B1, C2, D3, E4)\n",
    "    label_encoder = LabelEncoder()\n",
    "    y = label_encoder.fit_transform(y)\n",
    "\n",
    "    # 3. \n",
    "    X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=test_size, random_state=random_state, stratify=y)\n",
    "\n",
    "    print(f\"Training set: {X_train.shape}, Testing set: {X_test.shape}\")\n",
    "\n",
    "    return X_train, X_test, y_train, y_test\n",
    "\n",
    "# \n",
    "data_folder = r\"data\\GZA_5\\processed_data\"\n",
    "\n",
    "# \n",
    "X_train, X_test, y_train, y_test = load_data(data_folder)\n",
    "\n",
    "# \n",
    "print(\"X_train shape:\", X_train.shape)\n",
    "print(\"X_test shape:\", X_test.shape)\n",
    "print(\"y_train shape:\", y_train.shape)\n",
    "print(\"y_test shape:\", y_test.shape)\n"
