    "import numpy as np\n",
    "import pandas as pd\n",
    "import os\n",
    "from scipy.signal import welch\n",
    "import scipy.stats as stats\n",
    "import re\n",
    "# \n",
    "fs = 1000  #  (Hz)\n",
    "window_size = 200  # 200ms \n",
    "step_size = 100  # 100ms \n",
    "\n",
    "# A  B  C  D  E \n",
    "gesture_labels = [\"A\", \"B\", \"C\", \"D\", \"E\"]\n",
    "\n",
    "def extract_features(segment):\n",
    "    \"\"\"\n",
    "     EMG  IMU  15  (15, 10)\n",
    "    \"\"\"\n",
    "    # ** EMG ()  IMU ()**\n",
    "    emg_signals = segment[:, :4]  # 4  EMG  (200, 4)\n",
    "    imu_signals = segment[:, 4:]  # 6  IMU  (200, 6)\n",
    "\n",
    "    def compute_emg_features(signal):\n",
    "        return np.array([\n",
    "            np.var(signal), np.mean(np.abs(signal)), np.sqrt(np.mean(signal**2)),\n",
    "            np.std(signal), np.mean(np.abs(np.diff(signal))), np.max(signal),\n",
    "            np.min(signal), np.sum(np.diff(signal) > 0), np.sum(np.diff(np.sign(signal)) != 0),\n",
    "            stats.kurtosis(signal), stats.skew(signal), np.sum(np.abs(np.diff(signal))),\n",
    "            np.sum(signal ** 2), np.log10(np.mean(signal**2) + 1e-10),\n",
    "            np.mean(welch(signal, fs=1000, nperseg=200)[1])\n",
    "        ])\n",
    "\n",
    "    def compute_imu_features(signal):\n",
    "        return np.array([\n",
    "            np.var(signal), np.mean(signal), np.sqrt(np.mean(signal**2)),\n",
    "            np.std(signal), np.max(signal), np.min(signal),\n",
    "            stats.kurtosis(signal), stats.skew(signal),\n",
    "            np.mean(np.abs(np.diff(signal))), np.sum(np.abs(np.diff(signal))),\n",
    "            np.sum(signal ** 2), np.log10(np.mean(signal**2) + 1e-10),\n",
    "            np.mean(welch(signal, fs=1000, nperseg=200)[1]), np.median(signal), np.ptp(signal)\n",
    "        ])\n",
    "\n",
    "    emg_features = np.array([compute_emg_features(emg_signals[:, i]) for i in range(4)]).T  # (15, 4)\n",
    "    imu_features = np.array([compute_imu_features(imu_signals[:, i]) for i in range(6)]).T  # (15, 6)\n",
    "\n",
    "    return np.concatenate((emg_features, imu_features), axis=1)  # (15, 10)\n",
    "\n",
    "def detect_gesture_segments(time_series, threshold=2000):\n",
    "    \"\"\"\"\"\"\n",
    "    time_diff = np.diff(time_series)  # \n",
    "    split_indices = np.where(time_diff > threshold)[0] + 1  # >2000ms\n",
    "    segments = [0] + list(split_indices) + [len(time_series)]  # \n",
    "    \n",
    "    # \n",
    "    print(f\"Detected gesture segments (indices): {segments}\")\n",
    "    \n",
    "    return segments\n",
    "\n",
    "\n",
    "def process_emg_data(input_folder, output_folder):\n",
    "    \"\"\" CSV \"\"\"\n",
    "    os.makedirs(output_folder, exist_ok=True)\n",
    "    csv_files = sorted(\n",
    "        [f for f in os.listdir(input_folder) if f.startswith(\"sensor_data\") and f.endswith(\".csv\")],\n",
    "        key=lambda x: int(re.search(r\"sensor_data(\\d+)\\.csv\", x).group(1))\n",
    "    )\n",
    "    # csv_files = sorted([f for f in os.listdir(input_folder) if f.endswith(\".csv\")])\n",
    "    all_features = []\n",
    "    all_labels = []\n",
    "\n",
    "    for file_idx, file_name in enumerate(csv_files):\n",
    "        file_path = os.path.join(input_folder, file_name)\n",
    "        print(f\"Processing {file_name} ({file_idx+1}/{len(csv_files)})\")\n",
    "\n",
    "        #  CSV \n",
    "        raw_data = pd.read_csv(file_path)\n",
    "\n",
    "        # \n",
    "        time_series = raw_data.iloc[:, 0].values  # \n",
    "        \n",
    "        # \n",
    "        segments_indices = detect_gesture_segments(time_series)\n",
    "\n",
    "        # Z-score \n",
    "        data = raw_data.iloc[:, 1:].astype(np.float64)  # \n",
    "        mean_vals = data.mean(axis=0)\n",
    "        std_vals = data.std(axis=0)\n",
    "        normalized_data = (data - mean_vals) / (std_vals + 1e-10)  # \n",
    "\n",
    "        raw_data.iloc[:, 1:] = normalized_data\n",
    "\n",
    "        # \n",
    "        gesture_idx = 0  #  A-E\n",
    "        for i in range(len(segments_indices) - 1):\n",
    "            start_idx = segments_indices[i]  # \n",
    "            end_idx = segments_indices[i+1]  # \n",
    "\n",
    "            if end_idx <= start_idx:  # \n",
    "                continue\n",
    "\n",
    "            segment = raw_data.iloc[start_idx:end_idx, 1:].values  # \n",
    "\n",
    "            # \n",
    "            windows = [\n",
    "                segment[j:j + window_size, :]\n",
    "                for j in range(0, len(segment) - window_size + 1, step_size)\n",
    "            ]\n",
    "            windows = np.array(windows)  # (num_windows, 200, 10)\n",
    "\n",
    "            # \n",
    "            batch_features = np.array([extract_features(w) for w in windows])  # (num_windows, 15, 10)\n",
    "\n",
    "            # \n",
    "            all_features.append(batch_features)\n",
    "            all_labels.extend([gesture_labels[gesture_idx % len(gesture_labels)]] * len(windows))  #  A-E \n",
    "            gesture_idx += 1  # \n",
    "\n",
    "    #  NumPy \n",
    "    all_features = np.vstack(all_features)  # (, 15, 10)\n",
    "    all_labels = np.array(all_labels)  # (,)\n",
    "\n",
    "    # \n",
    "    np.save(os.path.join(output_folder, \"features.npy\"), all_features)\n",
    "    np.save(os.path.join(output_folder, \"labels.npy\"), all_labels)\n",
    "\n",
    "    print(f\"Feature extraction complete! Shape: {all_features.shape}\")\n",
    "    print(f\"Labels saved: {all_labels.shape}\")\n",
    "\n",
    "# \n",
    "root = r\"data\\GZA_5\"\n",
    "input_folder = root\n",
    "output_folder = os.path.join(root, \"processed_data\")\n",
    "process_emg_data(input_folder, output_folder)\n",
    "\n",
    "\n"
   ]
